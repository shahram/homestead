	xmlns:mvc="sap.ui.core.mvc"
    xmlns:f="sap.f">
	<App
		id="app"
		class="app"
    >
		<f:FlexibleColumnLayout
			id="layout"
			layout="{app>/layout}"
			backgroundDesign="Translucent"
        >
		</f:FlexibleColumnLayout>
	</App>
</mvc:View>